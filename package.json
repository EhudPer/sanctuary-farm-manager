{
  "name": "sanctuary-farm-manager",
  "version": "1.0.6",
  "description": "manage your sanctuary farm easily",
  "main": "server.js",
  "scripts": {
    "start": "NODE_ENV=production && npm install && node server.js",
    "heroku-postbuild": "cd client && npm install && npm run build",
    "server": "NODE_ENV=development DEBUG=express:* nodemon nodemon --ignore 'gcpconfig.json' server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "author": "Ehud Perlman",
  "license": "ISC",
  "dependencies": {
    "@google-cloud/storage": "^7.17.3",
    "bcryptjs": "^3.0.3",
    "body-parser": "^1.19.0",
    "concurrently": "^5.0.2",
    "express": "^5.1.0",
    "is-empty": "^1.2.0",
    "jsonwebtoken": "^9.0.2",
    "jwt-decode": "^2.2.0",
    "mongodb": "^7.0.0",
    "mongoose": "^9.0.0",
    "multer": "^2.0.2",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.0",
    "validator": "^13.15.23"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^7.1.2",
    "html-webpack-plugin": "^5.6.5",
    "nodemon": "^2.0.2",
    "style-loader": "^4.0.0",
    "webpack-cli": "^6.0.1"
  }
}
